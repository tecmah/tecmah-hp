{
  "ci": {
    "collect": {
      "staticDistDir": "./dist",
      "url": [
        "/",
        "/about",
        "/services/ai-data",
        "/case-studies",
        "/contact"
      ],
      "numberOfRuns": 3,
      "settings": {
        "preset": "desktop",
        "emulatedFormFactor": "desktop",
        "throttlingMethod": "devtools"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 0.6
          }
        ],
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "categories:best-practices": [
          "error",
          {
            "minScore": 0.95
          }
        ],
        "categories:seo": [
          "error",
          {
            "minScore": 0.95
          }
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
